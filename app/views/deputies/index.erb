<%= will_paginate collection %>
<table>
  <% collection.each do |deputy| %>
    <tr <%= error_attribute(deputy) %>>
      <td><%= link_to_object deputy %></td>
      <td>Last report <%= time_ago deputy.last_report_at %></td>
      <td><%= link_to_delete deputy, :text => 'x' %></td>
    </tr>
  <% end %>
</table>
<%= will_paginate collection %>