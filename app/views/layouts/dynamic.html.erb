<!doctype html>
<html xmlns:ng="http://angularjs.org/">
<head>
  <meta charset="utf-8">
  <title>Sheriff</title>
  <%= stylesheet_link_tag "application" %>
  <%= csrf_meta_tag %>
</head>










<body ng:controller="SheriffController">

<%= render 'layouts/topnav' %>


<div class="container" style='margin-top:40px;'>
  <div class="row">
    <div id='sidebar' class='span3' style='min-height:200px'>

    </div>
    <div id='main_content' class="span9">
      <div class="row">
        <div class='span9'>
          <ng:view></ng:view>
        </div>
      </div>
    </div>
    <div id="footer">
      <div class="block">
        <p>Copyright &copy; <%= Time.now.year %> Dawanda GmbH</p>
      </div>
    </div>
  </div>
</div>


  <script src="/assets/vendor/angular/angular-0.9.19.min.js" ng:autobind></script>
  <%= javascript_include_tag "application" -%>
</body>
</html>
