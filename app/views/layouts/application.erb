<!DOCTYPE html>
<html>
  <head>
    <title>Sheriff</title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :jquery %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <ul>
      <% [Group, Report, Deputy, Alert, Plugin].each do |type| %>
        <li>
          <%= link_to type.to_s.pluralize, type %>
        </li>
      <% end %>
    </ul>

    <%= yield(:content).presence || yield %>
  </body>
</html>